<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.digitalhouse.marveltime">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_marvel_time_branco"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activitys.SplashActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name=".Activitys.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activitys.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activitys.CadastroActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activitys.RecebePerguntasQuizActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activitys.RecyclerQuizActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activitys.RecyclerFavoritosActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activitys.RecyclerPersonagensActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activitys.PersonagensTelaActivity"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>
</manifest>