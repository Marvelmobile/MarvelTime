<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/corStringsBranco</item>
    </style>

    <style name="textoCadastro" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:fontFamily">Lato</item>
        <item name="android:textColor">#FFFFFFFF</item>
        <item name="android:textSize">18sp</item>
        <item name="android:lineSpacingMultiplier">1.29</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="textoBenVido">
        <item name="android:fontFamily">Lato</item>
        <item name="android:textColor">#FF000000</item>
        <item name="android:textSize">16sp</item>
        <item name="android:lineSpacingMultiplier">1.0</item>
    </style>

    <style name="titulo">
        <item name="android:fontFamily">Lato</item>
        <item name="android:textColor">#FF000000</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:lineSpacingMultiplier">1.0</item>
    </style>
  
    <style name="AppTheme.RoundedCornerMaterialButton" parent="Widget.AppCompat.Button.Colored">
        <item name="android:background">@drawable/round_button</item>
    </style>
</resources>


